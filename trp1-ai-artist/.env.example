# Example Environment Variables
# Copy this to .env and fill in your API keys

# Google APIs (Lyria, Veo, Imagen)
GEMINI_API_KEY=your_google_api_key_here

# AIMLAPI (MiniMax, Lyria2 via AIMLAPI)
AIMLAPI_KEY=your_aimlapi_key_here

# KlingAI Direct API
KLINGAI_API_KEY=your_kling_api_key_here
KLINGAI_SECRET_KEY=your_kling_secret_key_here

# Optional: Logging level
LOG_LEVEL=INFO
